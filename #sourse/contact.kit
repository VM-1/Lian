						<li class="contact-info-item">
							<p class="contact-item-tel"><span class="contact-item-bold">Tell:</span>123-456-7890</p>
						</li>
						<li class="contact-info-item">
							<p class="contact-item-tel"><span class="contact-item-bold">Fax:</span>123-456-7890</p>
						</li>
					</ul>
				</div>
				<form action="#" class="contact-form">
					<div class="contact-form-input"><input type="text" placeholder="Name"></div>
					<div class="contact-form-input"><input type="email" placeholder="Email"></div>
					<div class="contact-form-input"><input type="text" placeholder="Website"></div>
					<div class="contact-form-textarea">
						<textarea name="#" placeholder="Message"></textarea>
					</div>
					<button class="contact-form-btn">Send Message</button>
				</form>
			</article>
			<article class="map" id="map">
			</article>
